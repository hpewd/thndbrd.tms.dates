{
  "manifest_version": 2,
  "name": "MS Teams date/time/url extraction",
  "description": "Extract date, time and meeting-url from Microsoft teams invite from email file.",
  "version": "0.1",
  "author": "Hannes Dänschel, modified from Leon Wolfgang Rönnpagel",
  "applications": {
    "gecko": {
      "id": "hannes.daenschel@gmail.com",
      "strict_min_version": "78.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "message_display_action": {
    "default_title": "Copy Teams invite info ...",
    "theme_icons": [
      {
        "light": "images/calendar_white.png",
        "dark": "images/calendar_black.png",
        "size": 64
      }
    ]
  },
  "permissions": [
    "messagesRead",
    "downloads",
    "notifications",
    "clipboardWrite"
  ],
  "icons": {
    "64": "images/calendar_black.png"
  }
}